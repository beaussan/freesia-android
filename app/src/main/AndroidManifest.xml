<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeromelaquay.freesia">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/freesia"
        android:label="@string/app_name"
        android:roundIcon="@drawable/freesia"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BottomMenuActivity" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".TodoitemActivity"
            android:label="@string/title_activity_todoitem"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>